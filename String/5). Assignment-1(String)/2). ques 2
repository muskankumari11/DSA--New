// input a string of length n and count all the consonants in the given string
#include<iostream>
using namespace std;
int main(){
    int n;
    string s;
    cin>>n;
    cin>>s;
    int cnt=0;
    for(int i=0;i<(int)s.size();i++){
        if(s[i]=='a' || s[i]=='e' || s[i]=='i' || s[i]=='o' || s[i]=='u'){
            cnt++;
        }
    }
    cout<<n-cnt;
}
