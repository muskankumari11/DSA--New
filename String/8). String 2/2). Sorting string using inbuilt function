#include <iostream>
#include <string>
#include <algorithm>
using namespace std;

int main() {
    string s;
    getline(cin, s); 
    sort(s.begin(), s.end());
    cout << s; 
    return 0;
}

//here used getline(cin, s) in place of of cin>>s because it captures spaces also . in this space is character .
