//input a string and concatenate with its reverse string and print it.
//input:str="PWSkills"
//output:"PWSkillsslikSWP"
#include<iostream>
#include<string>
#include<algorithm>
using namespace std;
int main(){
    string str;
    cin>>str;
    string temp=str;
    reverse(temp.begin(),temp.end());
    str+=temp;
    cout<<"str is"<<str;
}
